#: ../packages/criterion/errors/errors.js:7
msgid "Old password cannot be reused."
msgstr "لا يمكن إعادة استخدام كلمة المرور القديمة."

#: ../packages/criterion/errors/errors.js:12
msgid "New password must contain at least one upper case letter."
msgstr "يجب أن تحتوي كلمة المرور الجديدة على حرف واحد على الأقل."

#: ../packages/criterion/errors/errors.js:17
msgid "New password must contain at least one lower case letter."
msgstr "يجب أن تحتوي كلمة المرور الجديدة على حرف واحد على الأقل."

#: ../packages/criterion/errors/errors.js:22
msgid "New password must contain at least one digit."
msgstr "يجب أن تحتوي كلمة المرور الجديدة على رقم واحد على الأقل."

#: ../packages/criterion/errors/errors.js:27
msgid "New password must contain at least one non alphanumeric character."
msgstr "يجب أن تحتوي كلمة المرور الجديدة على حرف غير أبجدي رقمي واحد على الأقل."

#: ../packages/criterion/errors/errors.js:32
msgid "New password must contain minimum {0} characters."
msgstr "يجب أن تحتوي كلمة المرور الجديدة على {0} حرف كحد أدنى."

#: ../packages/criterion/errors/errors.js:37
msgid "Old password you have provided is incorrect. Please provide valid password."
msgstr "كلمة المرور القديمة التي قدمتها غير صحيحة. يرجى تقديم كلمة مرور صالحة."

#: ../packages/criterion/errors/errors.js:42
msgid "The authentication code you entered is incorrect."
msgstr "رمز المصادقة الذي أدخلته غير صحيح."

#: ../packages/criterion/errors/errors.js:47
msgid "Entity does not exist. Table name: {0}, id: {1}"
msgstr "الكيان غير موجود. اسم الجدول: {0} ، المعرف: {1}"

#: ../packages/criterion/errors/errors.js:52
msgid "Failed to delete {0}. Please remove {1} references to this item."
msgstr "فشل في حذف {0} . يرجى إزالة {1} الإشارات إلى هذا العنصر."

#: ../packages/criterion/errors/errors.js:57
msgid "Invalid xml structure: <BR/> {0}"
msgstr "بنية XML غير صالحة: <BR/> {0}"

#: ../packages/criterion/errors/errors.js:62
msgid "Invalid app endpoint: {0}"
msgstr "نقطة نهاية التطبيق غير صالحة: {0}"

#: ../packages/criterion/errors/errors.js:67
msgid "Can't generate file. Employer {0} settings do not have tax filing client key."
msgstr "لا يمكن إنشاء ملف. لا تحتوي إعدادات صاحب العمل {0} مفتاح عميل حفظ الضريبة."

#: ../packages/criterion/errors/errors.js:72
msgid "Can't generate file. Employer {0} settings do not have employer type code."
msgstr "لا يمكن إنشاء ملف. إعدادات صاحب العمل {0} لا تحتوي على كود نوع صاحب العمل."

#: ../packages/criterion/errors/errors.js:77
msgid "Invalid state code {0} found for work location {1}."
msgstr "تم العثور على رمز حالة غير صالح {0} لموقع العمل {1} ."

#: ../packages/criterion/errors/errors.js:82
msgid "Employee's id '{0}' should not be longer than 20 characters."
msgstr "يجب ألا يزيد معرف الموظف '{0}' عن 20 حرفًا."

#: ../packages/criterion/errors/errors.js:87
msgid "Person's address 1 '{0}' should not be longer than 40 characters."
msgstr "يجب ألا يزيد عنوان الشخص 1 '{0}' عن 40 حرفًا."

#: ../packages/criterion/errors/errors.js:92
msgid "Person's address 2 '{0}' should not be longer than 30 characters."
msgstr "يجب ألا يزيد عنوان الشخص 2 '{0}' عن 30 حرفًا."

#: ../packages/criterion/errors/errors.js:97
msgid "Person's city '{0}' should not be longer than 28 characters."
msgstr "يجب ألا تتجاوز مدينة الشخص {0} 28 حرفًا."

#: ../packages/criterion/errors/errors.js:102
msgid "Person's state code '{0}' must be equal to 2 characters."
msgstr "يجب أن يكون كود حالة الشخص '{0}' مساويا لحرفين."

#: ../packages/criterion/errors/errors.js:107
msgid "Zip's code '{0}' must be equal to 5 characters."
msgstr "يجب أن يكون رمز Zip '{0}' مساويًا لـ 5 أحرف."

#: ../packages/criterion/errors/errors.js:112
msgid "Zip's ext '{0}' must be equal to 4 characters."
msgstr "يجب أن تكون قيمة الرمز البريدي '{0}' مساوية لـ 4 أحرف."

#: ../packages/criterion/errors/errors.js:117
msgid "Person's state description '{0}' should not be longer than 23 characters."
msgstr "يجب ألا يزيد وصف حالة الشخص '{0}' عن 23 حرفًا."

#: ../packages/criterion/errors/errors.js:122
msgid "The attribute 1 value '{0}' in detail in country code table must be equal to 2 characters."
msgstr "يجب أن تكون قيمة السمة 1 '{0}' بالتفصيل في جدول كود البلد مساوية لحرفين."

#: ../packages/criterion/errors/errors.js:127
msgid "Work Location's code '{0}' should not be longer than 35 characters."
msgstr "يجب ألا يزيد رمز موقع العمل '{0}' عن 35 حرفًا."

#: ../packages/criterion/errors/errors.js:132
msgid "Work Location's description '{0}' should not be longer than 35 characters."
msgstr "يجب ألا يزيد وصف موقع العمل '{0}' عن 35 حرفًا."

#: ../packages/criterion/errors/errors.js:137
msgid "Work Location's address 1 '{0}' should not be longer than 30 characters."
msgstr "يجب ألا يزيد عنوان موقع العمل 1 '{0}' عن 30 حرفًا."

#: ../packages/criterion/errors/errors.js:142
msgid "Work Location's address 2 '{0}' should not be longer than 25 characters."
msgstr "يجب ألا يزيد عنوان موقع العمل 2 '{0}' عن 25 حرفًا."

#: ../packages/criterion/errors/errors.js:147
msgid "Work Location's city '{0}' should not be longer than 28 characters."
msgstr "يجب ألا تزيد مدينة موقع العمل '{0}' عن 28 حرفًا."

#: ../packages/criterion/errors/errors.js:152
msgid "Work Location's state code '{0}' must be equal to 2 characters."
msgstr "يجب أن يكون كود حالة موقع العمل '{0}' مساوياً لحرفين."

#: ../packages/criterion/errors/errors.js:157
msgid "Employer's settings tax filling client key '{0}' should not be longer than 100 characters."
msgstr "يجب ألا يزيد طول مفتاح الضريبة على إعدادات صاحب العمل '{0}' عن 100 حرف."

#: ../packages/criterion/errors/errors.js:162
msgid "Employer's legal name '{0}' should not be longer than 40 characters."
msgstr "يجب ألا يتجاوز الاسم القانوني لصاحب العمل '{0}' 40 حرفًا."

#: ../packages/criterion/errors/errors.js:167
msgid "Can't transfer data. Data transfer {0} contains invalid file."
msgstr "لا يمكن نقل البيانات. يحتوي نقل البيانات {0} على ملف غير صالح."

#: ../packages/criterion/errors/errors.js:172
msgid "\"fileCustom\" or \"fileStandard\" should be defined."
msgstr "يجب تحديد \"fileCustom \" أو \"fileStandard \"."

#: ../packages/criterion/errors/errors.js:177
msgid "Payroll batch ({0}) is being processed and cannot be changed."
msgstr "مجموعة الرواتب ( {0} ) قيد المعالجة ولا يمكن تغييرها."

#: ../packages/criterion/errors/errors.js:182
msgid "ZIP code should be at least {0} characters long."
msgstr "يجب أن يكون طول الرمز البريدي {0} حرفًا على الأقل."

#: ../packages/criterion/errors/errors.js:187
msgid "It's impossible to delegate workflow to yourself."
msgstr "من المستحيل تفويض سير العمل لنفسك."

#: ../packages/criterion/errors/errors.js:192
msgid "Workflow delegation's end date {1:date} can't be before start date {0:date}."
msgstr "لا يمكن أن يكون تاريخ انتهاء تفويض سير العمل {1: date} قبل تاريخ البدء {0: date}."

#: ../packages/criterion/errors/errors.js:197
msgid "Employee {0} is not a member of your team."
msgstr "الموظف {0} ليس عضوًا في فريقك."

#: ../packages/criterion/errors/errors.js:202
msgid "Invalid data. Employee {0} doesn't have primary work location assigned. Please contact your system administrator."
msgstr "بيانات غير صالحة. الموظف {0} ليس لديه موقع عمل أساسي معين. من فضلك تواصل مع مسؤول نظامك."

#: ../packages/criterion/errors/errors.js:207
msgid "Please provide state for Employer Location '{0}'."
msgstr "يرجى ذكر حالة صاحب العمل '{0}'."

#: ../packages/criterion/errors/errors.js:212
msgid "Incorrect date range. Effective date({0:date}) should be less than Expiration date({1:date})."
msgstr "نطاق التاريخ غير صحيح. يجب أن يكون التاريخ الفعلي ({0: date}) أقل من تاريخ انتهاء الصلاحية ({1: date})."

#: ../packages/criterion/errors/errors.js:217
msgid "Cannot update data. Data length is exceeded in column '{0}'."
msgstr "لا يمكن تحديث البيانات. تم تجاوز طول البيانات في العمود '{0}'."

#: ../packages/criterion/errors/errors.js:222
msgid "Cannot update data. Value length is exceeded in column '{0}'."
msgstr "لا يمكن تحديث البيانات. تم تجاوز طول القيمة في العمود '{0}'."

#: ../packages/criterion/errors/errors.js:227
msgid "Person with {0} email already exists. Please use another email."
msgstr "الشخص الذي لديه {0} بريد إلكتروني موجود بالفعل. يرجى استخدام بريد إلكتروني آخر."

#: ../packages/criterion/errors/errors.js:232
#: ../packages/criterion/errors/errors.js:237
msgid "Similar SalaryGrade record already exists in the database. Please provide unique salary grade."
msgstr "سجل الراتب المتماثل موجود بالفعل في قاعدة البيانات. يرجى تقديم درجة راتب فريدة."

#: ../packages/criterion/errors/errors.js:242
msgid "Incorrect formula. Coverage formula is not valid."
msgstr "صيغة غير صحيحة. صيغة التغطية غير صالحة."

#: ../packages/criterion/errors/errors.js:247
msgid "Incorrect formula. Premium formula is not valid."
msgstr "صيغة غير صحيحة. الصيغة المميزة غير صالحة."

#: ../packages/criterion/errors/errors.js:252
msgid "Incorrect formula. Dependent formula is not valid."
msgstr "صيغة غير صحيحة. الصيغة التابعة غير صالحة."

#: ../packages/criterion/errors/errors.js:257
msgid "Incorrect formula. Employee Contribution formula is not valid."
msgstr "صيغة غير صحيحة. صيغة مساهمة الموظف غير صالحة."

#: ../packages/criterion/errors/errors.js:262
msgid "Incorrect formula. Eligibility formula is not valid."
msgstr "صيغة غير صحيحة. صيغة الأهلية غير صالحة."

#: ../packages/criterion/errors/errors.js:267
msgid "Incorrect formula. Effective formula is not valid."
msgstr "صيغة غير صحيحة. الصيغة الفعالة غير صالحة."

#: ../packages/criterion/errors/errors.js:272
msgid "Incorrect formula. Expire formula is not valid."
msgstr "صيغة غير صحيحة. صيغة انتهاء الصلاحية غير صالحة."

#: ../packages/criterion/errors/errors.js:277
msgid "Payroll import settings are not defined. Please upload settings XML."
msgstr "لم يتم تحديد إعدادات استيراد كشوف المرتبات. يرجى تحميل الإعدادات XML."

#: ../packages/criterion/errors/errors.js:282
msgid "Payroll import settings has invalid XML, tag: {0}."
msgstr "تحتوي إعدادات استيراد كشوف المرتبات على XML غير صالح ، علامة: {0} ."

#: ../packages/criterion/errors/errors.js:287
msgid "Selected payroll import is already saved to batch. Please import another file."
msgstr "يتم حفظ استيراد كشوف المرتبات المحددة بالفعل إلى مجموعة. يرجى استيراد ملف آخر."

#: ../packages/criterion/errors/errors.js:292
msgid "Uploaded .csv file is empty. Please provide file of correct format."
msgstr "ملف .csv الذي تم تحميله فارغ. يرجى تقديم ملف بالتنسيق الصحيح."

#: ../packages/criterion/errors/errors.js:297
msgid "Uploaded .csv file or timesheet has no import details. Please provide file of correct format or check timesheet period."
msgstr "لا يحتوي ملف .csv أو الجدول الزمني الذي تم تحميله على تفاصيل عن الاستيراد. يرجى تقديم ملف بالتنسيق الصحيح أو التحقق من فترة الجدول الزمني."

#: ../packages/criterion/errors/errors.js:302
msgid "Uploaded .csv file doesn't contain Column {0}. Please provide file of correct format."
msgstr "لا يحتوي ملف .csv الذي تم تحميله على عمود {0} . يرجى تقديم ملف بالتنسيق الصحيح."

#: ../packages/criterion/errors/errors.js:307
msgid "Can't process payroll import. Employer with location with code {0} doesn't exist."
msgstr "لا يمكن معالجة استيراد كشوف المرتبات. صاحب العمل مع الموقع مع رمز {0} غير موجود."

#: ../packages/criterion/errors/errors.js:312
msgid "Can't process payroll import. Employer task with code {0} doesn't exist."
msgstr "لا يمكن معالجة استيراد كشوف المرتبات. مهمة صاحب العمل بالكود {0} غير موجودة."

#: ../packages/criterion/errors/errors.js:317
msgid "Employee deductions cannot overlap. Prior employee deduction will expire on {0:date}. Please close the prior deduction before creating a new one with the same dates."
msgstr "لا يمكن أن تتداخل استقطاعات الموظفين. سينتهي خصم الموظف السابق في {0: date}. يرجى إغلاق الخصم السابق قبل إنشاء حساب جديد بنفس التواريخ."

#: ../packages/criterion/errors/errors.js:322
msgid "The range for Pay Date Days After is [{0}, {1}]. Please specify the date that falls into this date range."
msgstr "النطاق لـ Pay Date Days After هو [ {0} ، {1} ]. يرجى تحديد التاريخ الذي يقع في هذا النطاق الزمني."

#: ../packages/criterion/errors/errors.js:327
msgid "Income {0} is already used in the batch. Please select another Income type."
msgstr "الدخل {0} مستخدم بالفعل في الدفعة. يرجى اختيار نوع دخل آخر."

#: ../packages/criterion/errors/errors.js:332
msgid "Pay rate({0}) for assignment can't fall outside the min/max({1}/{2}) range of the salary grade."
msgstr "لا يمكن أن يكون معدل الدفع ( {0} ) للواجب خارج نطاق الحد الأدنى / الحد الأقصى {1} / {2} ) من درجة الراتب."

#: ../packages/criterion/errors/errors.js:337
msgid "Time format {0} is incorrect. Timesheet detail hours should fall in range of {1} - {2}."
msgstr "تنسيق الوقت {0} غير صحيح. يجب أن تقع ساعات تفاصيل الجدول الزمني في نطاق {1} - {2} ."

#: ../packages/criterion/errors/errors.js:342
msgid "Time format {0} is incorrect. Timesheet detail minutes should fall in range of {1} - {2}."
msgstr "تنسيق الوقت {0} غير صحيح. يجب أن تقع دقائق تفاصيل الجدول الزمني في نطاق {1} - {2} ."

#: ../packages/criterion/errors/errors.js:347
msgid "Time format {0} is incorrect. Timesheet detail units should be greater or equal 0."
msgstr "تنسيق الوقت {0} غير صحيح. يجب أن تكون وحدات تفاصيل الجدول الزمني أكبر أو تساوي 0."

#: ../packages/criterion/errors/errors.js:352
msgid "Time off is not added. Employee has Regular Closed Day or Holiday in specified period."
msgstr "لا يتم إضافة إجازة. الموظف لديه يوم مغلق منتظم أو عطلة في فترة محددة."

#: ../packages/criterion/errors/errors.js:357
msgid "Employee cannot be terminated. Employee has active additional assignments. Please remove all the references to the employee."
msgstr "لا يمكن إنهاء الموظف. الموظف لديه مهام إضافية نشطة. يرجى إزالة جميع الإشارات إلى الموظف."

#: ../packages/criterion/errors/errors.js:362
msgid "You cannot terminate a terminated employee."
msgstr "لا يمكنك إنهاء موظف تم إنهائه."

#: ../packages/criterion/errors/errors.js:367
msgid "Termination date {0:date} should be equal to or later than effective date of last assignment detail<br>{1:date}."
msgstr "يجب أن يكون تاريخ الإنهاء {0: date} مساويًا أو أحدث من تاريخ نفاذ آخر تفاصيل مهمة <br> {1: date}."

#: ../packages/criterion/errors/errors.js:372
msgid "Can't update employee. Employee changes are in pending approval state. Workflow {0} should be completed first."
msgstr "لا يمكن تحديث الموظف. تغييرات الموظفين في حالة الموافقة المعلقة. يجب إكمال سير العمل {0} أولاً."

#: ../packages/criterion/errors/errors.js:377
msgid "Wrong benefit plan option. Non-manual benefit plan option is expected for Option Group {0}."
msgstr "خيار خطة منفعة خاطئة. خيار خطة المنافع غير اليدوية متوقع لمجموعة Option {0} ."

#: ../packages/criterion/errors/errors.js:382
msgid "Incorrect candidate rating. Rating {0} should be in range [0,5] with 0.5 as a step."
msgstr "تصنيف مرشح غير صحيح. يجب أن يكون التصنيف {0} في النطاق [0،5] مع 0.5 كخطوة."

#: ../packages/criterion/errors/errors.js:387
msgid "Employer {0} doesn't have National Identifier."
msgstr "صاحب العمل {0} ليس لديه معرّف وطني."

#: ../packages/criterion/errors/errors.js:392
msgid "Can't set this location as primary. Primary work location for this employer already exists. Only one primary work location per employer is allowed."
msgstr "لا يمكن تعيين هذا الموقع على أنه أساسي. يوجد موقع العمل الأساسي لصاحب العمل هذا بالفعل. يُسمح فقط لموقع عمل أساسي واحد لكل صاحب عمل."

#: ../packages/criterion/errors/errors.js:397
msgid "Please provide state in address for person {0}."
msgstr "يرجى تقديم الدولة في العنوان للشخص {0} ."

#: ../packages/criterion/errors/errors.js:402
msgid "Person {0} has several primary addresses, it's allowed to have just one."
msgstr "لدى الشخص {0} عدة عناوين أساسية ، يُسمح له بالحصول على عنوان واحد فقط."

#: ../packages/criterion/errors/errors.js:407
msgid "Person {0} has several mailing addresses, it's allowed to have just one."
msgstr "لدى الشخص {0} عدة عناوين بريدية ، يُسمح أن يكون لديه واحد فقط."

#: ../packages/criterion/errors/errors.js:412
msgid "Person {0} does not have primary address, but should have at least one."
msgstr "الشخص {0} ليس لديه عنوان أساسي ، لكن يجب أن يكون لديه عنوان واحد على الأقل."

#: ../packages/criterion/errors/errors.js:417
msgid "Employee {0} has several primary work locations, it's allowed to have just one."
msgstr "لدى الموظف {0} العديد من مواقع العمل الأساسية ، يُسمح أن يكون لديه موقع واحد فقط."

#: ../packages/criterion/errors/errors.js:422
msgid "Employee {0} does not have primary work location, but should have at least one."
msgstr "لا يوجد لدى الموظف {0} موقع العمل الأساسي ، لكن يجب أن يكون لديه موقع واحد على الأقل."

#: ../packages/criterion/errors/errors.js:427
msgid "Please provide country 2-letter code (code table detail attribute 1) for person {0}."
msgstr "يرجى تقديم كود البلد المكون من حرفين (السمة التفصيلية لجدول الأكواد 1) للشخص {0} ."

#: ../packages/criterion/errors/errors.js:432
msgid "Please provide zip code in address for person {0}."
msgstr "يرجى تقديم الرمز البريدي في العنوان للشخص {0} ."

#: ../packages/criterion/errors/errors.js:437
msgid "Zip code is empty in work location {0}."
msgstr "الرمز البريدي فارغ في موقع العمل {0} ."

#: ../packages/criterion/errors/errors.js:442
msgid "Address 1 is empty in work location {0}."
msgstr "العنوان 1 فارغ في موقع العمل {0} ."

#: ../packages/criterion/errors/errors.js:447
msgid "City is empty in work location {0}."
msgstr "المدينة فارغة في موقع العمل {0} ."

#: ../packages/criterion/errors/errors.js:452
msgid "Can't update time off. You don't have enough comp time earned."
msgstr "لا يمكن تحديث إجازة. ليس لديك ما يكفي من شركات الوقت المكتسبة."

#: ../packages/criterion/errors/errors.js:457
msgid "Time off duration cannot exceed employee average hours."
msgstr "لا يمكن أن تتجاوز مدة إجازة الموظف متوسط ساعات العمل."

#: ../packages/criterion/errors/errors.js:462
msgid "ACA data for Employer {0} and year {1} has already been generated."
msgstr "تم بالفعل إنشاء بيانات ACA الخاصة بصاحب العمل {0} والسنة {1} ."

#: ../packages/criterion/errors/errors.js:467
msgid "Benefit plan rate with code '{0}' and tier '{1}' already exists."
msgstr "معدل خطة المنفعة بالكود '{0}' والمستوى '{1}' موجود بالفعل."

#: ../packages/criterion/errors/errors.js:472
msgid "Invalid View Number {0}. View number should be in range [1,3]. Please provide correct number."
msgstr "رقم العرض غير صالح {0} . يجب أن يكون رقم العرض في النطاق [1،3]. يرجى تقديم الرقم الصحيح."

#: ../packages/criterion/errors/errors.js:477
msgid "This dashboard view is already in use. Please choose another dashboard view."
msgstr "طريقة عرض لوحة المعلومات هذه قيد الاستخدام بالفعل. الرجاء اختيار طريقة عرض لوحة معلومات أخرى."

